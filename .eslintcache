.listDeleted\n  ? <div>\n    <p>BudGit Deleted</p>\n    <Link to=\"/\"><p>Back to my BudGitz</p></Link>\n  </div>\n  :   <div>\n  <h3>Delete {listName}?</h3>\n  <AppForm onSubmit={this.handleClickDelete}\n  onCancel={this.handleClickCancel}>\n  <button type=\"cancel\" className='buttons' onClick={this.handleClickCancel}>No</button>\n  <button className='buttons' type = \"submit\">Yes</button>\n  </AppForm>\n  </div>\n\n\n    return(\n        <section className='delete-list'>\n        {display}\n        </section>\n\n\n    )\n}\n}\n\nDeleteList.defaultProps = {\n    item: \"\",\n    list: \"\"\n}\nDeleteList.propTypes = {\n    item: PropTypes.string.isRequired,\n    list: PropTypes.string.isRequired\n  }","/Users/ozzieharb/Projects/budgitz-client/src/ExpandButton/ExpandButton.js",[],"/Users/ozzieharb/Projects/budgitz-client/src/EditItem/EditItem.js",["91"],{"ruleId":"92","replacedBy":"93"},{"ruleId":"94","replacedBy":"95"},{"ruleId":"96","severity":1,"message":"97","line":10,"column":23,"nodeType":"98","messageId":"99","endLine":10,"endColumn":28},{"ruleId":"96","severity":1,"message":"100","line":7,"column":8,"nodeType":"98","messageId":"99","endLine":7,"endColumn":18},{"ruleId":"96","severity":1,"message":"101","line":11,"column":56,"nodeType":"98","messageId":"99","endLine":11,"endColumn":67},{"ruleId":"96","severity":1,"message":"102","line":3,"column":10,"nodeType":"98","messageId":"99","endLine":3,"endColumn":18},{"ruleId":"96","severity":1,"message":"103","line":12,"column":10,"nodeType":"98","messageId":"99","endLine":12,"endColumn":25},{"ruleId":"96","severity":1,"message":"104","line":113,"column":17,"nodeType":"98","messageId":"99","endLine":113,"endColumn":19},{"ruleId":"96","severity":1,"message":"105","line":12,"column":8,"nodeType":"98","messageId":"99","endLine":12,"endColumn":12},{"ruleId":"96","severity":1,"message":"103","line":8,"column":10,"nodeType":"98","messageId":"99","endLine":8,"endColumn":25},{"ruleId":"96","severity":1,"message":"101","li