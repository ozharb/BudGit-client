od: 'DELETE',\n    headers: {\n      'content-type': 'application/json'\n    },\n  })\n    .then(res => {\n      if (!res.ok)\n        return res.json().then(e => Promise.reject(e))\n      \n    })\n    .then(() => {\n\n     \n      this.context.deleteItem(itemId)\n\n    })\n    .catch(error => {\n      console.error({ error })\n    })\n}\n\n\nrender(){\n    const itemId = parseInt(this.props.id)\n    const { items=[] } = this.context\n    const item = findItem(items, itemId)\n    const calcButton = item.calc\n    ? 'Remove'\n     : 'Put Back'\n\n  const { name, id, price, quantity, date_made, calc } = this.props\n  return (\n    <div className='Item'>\n      <h2 className='Item__name'>\n        <Link to={`/item/${id}`}>\n          {name}\n        {' | '}\n          cost: ${price}\n        {' | '}\n          qty: {quantity}\n        </Link>\n      </h2>\n\n      <button className = 'item_calc'\n      type='radio' name = 'calc' \n      onClick={this.handleChangeCalc} \n      \n      value = {calc}> \n      \n      {calcButton}\n      \n      </button>\n\n      <button className='item__delete' \n      type='button'\n      onClick={this.handleClickDelete}\n      >\n         \n        {/* <FontAwesomeIcon icon={faSkullCrossbones}  /> */}\n        {' '}\n        Delete\n      </button>\n      <div className='Item__date'>\n        <div className='Item__date_made'>\n          Added:\n          {' '}\n          <span className='date_made'>\n            {\nformat(toDate(date_made), 'EEEE MM/dd/yyyy h:mm aaaaa\\'m\\' z', { timeZone : 'America/Los_Angeles' })}\n          </span>\n        </div>\n      </div>\n    </div>\n  )\n}\n}\nItem.defaultProps = {\n  name:\"\",\n  id: \"\",\n}\n\nItem.propTypes = {\n  props: PropTypes.shape({\n    id: PropTypes.isRequired,\n    item_name: PropTypes.string.isRequired,\n  }),\n\n}\nexport default Item","/Users/ozzieharb/Projects/budgitz-client/src/app-helpers.js",[],"/Users/ozzieharb/Projects/budgitz-client/src/SummaryList/SummaryList.js",